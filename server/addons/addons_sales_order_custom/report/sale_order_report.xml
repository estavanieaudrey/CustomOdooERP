<odoo>
    <template id="report_draft_agreement">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <div class="page">
                    <h3>Draft Perjanjian</h3>
                    <p>Nama Customer: <t t-esc="o.partner_id.name"/></p>
                    <p>Judul: <t t-esc="o.agreement_title"/></p>
                    <p>Tanggal Kedaluwarsa: <t t-esc="o.validity_date"/></p>
                </div>
            </t>
        </t>
    </template>
</odoo>
